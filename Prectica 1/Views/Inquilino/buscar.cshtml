@model Prectica_1.Models.Inquilino;
@{
    ViewData["Title"] = "Busco nuevo Inquilino";
}
<div class="text-center">
    <h1 class="display-3">Crear Persona</h1>

</div>
<div class="container">
    <div class="col-md-6 mx-auto">
        <form asp-action="buscarDNI" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="dni" class="control-label">Dni</label>
                <input asp-for="dni" class="form-control" />
                <span asp-validation-for="dni" class="text-danger"></span>
            </div>

            <div class="mt-3">
                <button type="submit " value="buscarDNI" class="btn btn-primary btn-sm">Buscar</button>
                <a href="Index" class="btn btn-primary btn-sm">Cancelar</a>
            </div>
        </form>
    </div>
</div>